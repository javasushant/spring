<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<bean id="home" class="org.springframework.web.servlet.view.InternalResourceView">
	<property name="url" value="WEB-INF/pages/namaste.jsp"></property>
	</bean>
	<bean id="result" class="org.springframework.web.servlet.view.InternalResourceView">
	<property name="url" value="WEB-INF/pages/show_result.jsp"></property>
	</bean>
	<bean id="about_us" class="org.springframework.web.servlet.view.InternalResourceView">
	<property name="url" value="WEB-INF/pages1/about_us.jsp"></property>
	</bean>
</beans>
